/* components/search/search.wxss */
.my-search-container {
  height: 50px;
  background-color: #e54847;
  display: flex;
  align-items: center;
  padding: 0 10px;
}

.my-search-box {
  height: 36px;
  background-color: #FFFFFF;
  border-radius: 18px;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.placeholder {
  font-size: 15px;
  margin-left: 5px;
}

.search {
  width: 100%;
}

.my-search-box .van-search {
  height: 36px;
}