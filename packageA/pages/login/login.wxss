/* packageA/pages/login/login.wxss */
page {
  background-image: linear-gradient(to top, #fdcbf1 0%, #fdcbf1 1%, #e6dee9 100%);
}
.register {
  width: 600rpx;
  margin: auto;
  margin-top: 45%;
  padding: 120rpx 5rpx 0;
  border-radius: 5px;
  background-color: #fff;
  position: relative;
}

.login {
  width: 600rpx;
  margin: auto;
  margin-top: 45%;
  padding: 5rpx;
  border-radius: 5px;
  background-color: #fff;
  position: relative;
}

.photo {
  position: absolute;
  top: -50%;
  left: 50%;
  transform: translate(-50%, 30%);
  z-index: 999;
}

.photo image{
  width: 230rpx;
  height: 230rpx;
  border-radius: 50%;
}

.btn {
  width: 610rpx;
  margin: auto;
  margin-top: 50rpx;
  background-image: linear-gradient(120deg, #f6d365 0%, #fda085 100%);
  border-radius: 100px;
  color: #fff;
  font-weight: 700;
}